<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<regexes>
    <regex id="LOGIN_REGEX">\w{5,20}</regex>
    <regex id="EMAIL_REGEX">[a-zA-Z0-9_\.]+\@[a-zA-Z0-9\.]{2,20}\.\w{2,4}</regex>
    <regex id="PASSWORD_REGEX">.{5,20}</regex>
    <regex id="ID_REGEX">\d+</regex>
    <regex id="WS_MESSAGE_REGEX">(\d\.\d)|(\{\"message\"\:\".{1,1000}\"\})</regex>
</regexes>